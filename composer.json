{
  "require": {
    "craftcms/cms": "3.6.4.1",
    "craftcms/contact-form": "2.2.7",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/feed-me": "4.3.5.1",
    "ether/seo": "3.6.7",
    "matt-west/craft-recaptcha": "1.5.1",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "vaersaagod/dospaces": "1.1.3",
    "vlucas/phpdotenv": "^3.4.0",
    "wrav/simplesharing": "1.0.8"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
